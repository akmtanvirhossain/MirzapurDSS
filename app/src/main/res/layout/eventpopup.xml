<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/popup"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

	    <LinearLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:orientation="horizontal" style="@style/rowStyle1">
	            <TableLayout 
				android:id="@+id/firsttab"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent">
				
		        <TableRow  android:gravity="left">
			        <TextView android:id="@+id/VlblQSNo" android:layout_column="0" android:text="Serial No" android:layout_width="90sp" android:layout_height="wrap_content" android:textSize="20sp" />
			        <EditText android:id="@+id/txtQSNo" android:layout_column="1" android:enabled="false" android:maxLength="2" android:layout_width="50sp" android:inputType="text" android:layout_height="wrap_content" style="@style/customEdit" android:layout_marginRight="10sp" android:textSize="20sp" />
			        <TextView android:id="@+id/lblName" android:layout_column="2" android:text="" android:layout_width="250sp" android:layout_height="wrap_content" android:textSize="20sp" />
				<Button
		        android:id="@+id/cmdEvent" android:layout_column="3"
		        android:layout_width="60sp"
		        android:layout_height="wrap_content"
		        android:text="EV" />				
							        			        
		        </TableRow>	
	        </TableLayout>
	        
	    </LinearLayout>
<View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />	    	    
	    <LinearLayout
	        android:id="@+id/secEvType" android:paddingLeft="10sp" android:paddingTop="20sp" android:background="#ffffff"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal">

	        <TextView android:id="@+id/VlblQEType" android:text="Event Type" android:layout_width="120sp" android:layout_height="wrap_content" android:textSize="20sp" />

			<Spinner
			    android:id="@+id/EvType"
			    android:layout_width="150dp"
			    android:layout_height="wrap_content"
			    android:layout_marginBottom="20sp"
			    android:layout_marginRight="10sp"
			    android:drawSelectorOnTop="true"
			    android:prompt="@string/planet_prompt"
			    android:textSize="25sp" />
	        
	        <TextView android:id="@+id/VlblQPNo" android:text="PNo" android:layout_width="60sp" android:layout_height="wrap_content" android:textSize="20sp" />
	        <EditText android:id="@+id/txtQPNo" android:enabled="false" android:maxLength="8" android:layout_width="150sp" android:inputType="text" android:layout_height="wrap_content" style="@style/customEdit" android:layout_marginRight="10sp" android:textSize="20sp" />

	    </LinearLayout>
<View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />	    
		<LinearLayout
		    android:id="@+id/secEvDate" android:paddingLeft="10sp"  android:background="#ffffff"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal" >
	
			<TextView
			    android:id="@+id/lblEvDate"
			    android:layout_width="120sp"			    
			    android:layout_height="wrap_content"
			    android:text="Event Date" />
	
			<EditText
			    android:id="@+id/EvDate" android:layout_marginBottom="20sp" style="@style/customEdit"
			    android:layout_marginRight="10sp"
			    android:layout_width="130sp"
			    android:layout_height="wrap_content"
			    android:ems="10"
			    android:maxLength="10"
			    android:inputType="date"  android:digits="0123456789/">
			</EditText>
			<ImageButton android:id="@+id/btnEvDate" android:layout_width="40sp" android:layout_height="40sp" android:src="@drawable/calender" android:layout_gravity="center" />
			<TextView
			    android:id="@+id/lblCode"
			    android:layout_width="60dp"
			    android:layout_height="wrap_content"
			    android:text="Code" />
			
			<EditText
			    android:id="@+id/txtCode" android:layout_marginBottom="20sp" style="@style/customEdit"
			    android:layout_marginRight="10sp"
			    android:layout_width="150sp"
			    android:layout_height="wrap_content"
			    android:ems="10"
			    android:maxLength="20"
			    android:inputType="number" >
			</EditText>
			<Spinner
			    android:id="@+id/txtCodeList" android:layout_marginBottom="20sp"
			    android:layout_marginRight="10sp"
			    android:layout_width="200dp"
			    android:layout_height="wrap_content"
			    android:drawSelectorOnTop="true"
			    android:prompt="@string/planet_prompt"
			    android:textSize="20sp" />
		</LinearLayout>
<View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />		
		<LinearLayout android:background="#ffffff"
		    android:id="@+id/secSp" android:paddingLeft="10sp"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal" >
		    
			<TextView
			    android:id="@+id/lblSpNo"
			    android:layout_width="120sp"
			    android:layout_height="wrap_content"
			    android:text="Spouse's No" />
	
			<EditText
			    android:id="@+id/SpNo" android:layout_marginBottom="20sp" style="@style/customEdit"
			    android:layout_marginRight="10sp"
			    android:layout_width="150sp"
			    android:layout_height="wrap_content"
			    android:ems="10" android:maxLength="2" android:inputType="number"/>
	
		</LinearLayout>
<View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />		
	<LinearLayout android:background="#ffffff"
	    android:id="@+id/secPOR" android:paddingLeft="10sp"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal" >

		<TextView
		    android:id="@+id/lblPOR"
		    android:layout_width="120sp"
		    android:layout_height="wrap_content"
		    android:text="Outcome Res." />

			<Spinner
			    android:id="@+id/txtPOR" android:layout_marginBottom="20sp"
			    android:layout_marginRight="10sp"
			    android:layout_width="200dp"
			    android:layout_height="wrap_content"
			    android:drawSelectorOnTop="true"
			    android:prompt="@string/planet_prompt"
			    android:textSize="20sp" />
	</LinearLayout>
<View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />	
	<LinearLayout android:background="#ffffff"
	    android:id="@+id/secDeliv" android:paddingLeft="10sp"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal" >

		<TextView
		    android:id="@+id/lbl43"
		    android:layout_width="120sp"
		    android:layout_height="wrap_content"
		    android:text="Del. Place" />

<!--			<Spinner
			    android:id="@+id/txt43" android:layout_marginBottom="20sp"
			    android:layout_marginRight="10sp"
			    android:layout_width="150dp"
			    android:layout_height="wrap_content"
			    android:drawSelectorOnTop="true"
			    android:prompt="@string/planet_prompt"
			    android:textSize="20sp" />-->
			<Spinner android:id="@+id/txt43" android:spinnerMode="dialog"
				android:drawSelectorOnTop="true" android:layout_width="150dp"
				android:layout_height="wrap_content" android:layout_weight="0"
				android:prompt="@string/pop_prompt" android:textSize="20sp" />
		<TextView
		    android:id="@+id/lbl44"
		    android:layout_width="60sp"
		    android:layout_height="wrap_content"
		    android:text="Atten." />

			<Spinner
			    android:id="@+id/txt44" android:layout_marginBottom="20sp"
			    android:layout_marginRight="10sp"
			    android:layout_width="150dp"
			    android:layout_height="wrap_content"
			    android:drawSelectorOnTop="true"
			    android:prompt="@string/planet_prompt"
			    android:textSize="20sp" />
	</LinearLayout>
<View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />	
	<LinearLayout android:background="#ffffff"
		    android:id="@+id/secLMP" android:paddingLeft="10sp"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal" >
		<TextView
		    android:id="@+id/lblLMP" android:layout_marginBottom="20sp"
			android:layout_marginRight="10sp"
		    android:layout_width="120sp"
		    android:layout_height="wrap_content"
		    android:text="LMP" />

		<EditText
		    android:id="@+id/LmpDt" style="@style/customEdit"
		    android:layout_width="130sp"
		    android:layout_height="wrap_content"
		    android:ems="10" android:maxLength="10"
		    android:inputType="date" android:digits="0123456789/"/>
		<ImageButton android:id="@+id/btnLmpDt" android:visibility="gone" android:layout_width="40sp" android:layout_height="40sp" android:src="@drawable/calender" android:layout_gravity="center" />

	</LinearLayout>	
<View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />

	<LinearLayout android:background="#ffffff"
	    android:id="@+id/secReason" android:paddingLeft="10sp"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical" >
		    <LinearLayout android:id="@+id/secQRAge" android:layout_width="fill_parent" android:layout_height="wrap_content" android:orientation="horizontal" style="@style/rowStyle0">		        
		        <TextView android:id="@+id/lblQRAge" android:text="Reason/Age" android:layout_width="150sp" android:layout_height="wrap_content" android:textSize="20sp" />
		        <EditText android:id="@+id/txtRAge" android:maxLength="2" android:layout_width="100sp" android:inputType="number" android:layout_height="wrap_content" style="@style/customEdit" android:layout_marginRight="10sp" android:textSize="20sp" />
		    </LinearLayout>
		
		    <LinearLayout android:id="@+id/secQEvDT" android:layout_width="fill_parent" android:layout_height="wrap_content" android:orientation="horizontal" style="@style/rowStyle0">		        
		        <TextView android:id="@+id/VlblQEvDT" android:text="Event Date" android:layout_width="150sp" android:layout_height="wrap_content" android:textSize="20sp" />
		        <EditText android:id="@+id/txtREvDT" android:layout_width="120sp" android:maxLength="10" android:inputType="date" android:digits="0123456789/" android:layout_height="wrap_content" style="@style/customEdit" android:layout_marginRight="10sp" android:textSize="20sp" />
				<ImageButton android:id="@+id/btnREvDT" android:layout_width="40sp" android:layout_height="40sp" android:src="@drawable/calender" android:layout_gravity="center" />
			</LinearLayout>
		
		    <LinearLayout android:id="@+id/secQEvReason" android:layout_width="fill_parent" android:layout_height="wrap_content" android:orientation="horizontal" style="@style/rowStyle0">		        
		        <TextView android:id="@+id/VlblQReason" android:text="Reason" android:layout_width="150sp" android:layout_height="wrap_content" android:textSize="20sp" />
		        <EditText android:id="@+id/txtRReason" android:maxLength="2" android:layout_width="100sp" android:inputType="number" android:layout_height="wrap_content" style="@style/customEdit" android:layout_marginRight="10sp" android:textSize="20sp" />
		   
		   </LinearLayout>
	</LinearLayout>
<View android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />
	
	<LinearLayout android:background="#f3f3f3"
		    android:id="@+id/secButton" android:paddingLeft="30sp"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content" android:paddingTop="20sp"
		    android:orientation="horizontal" android:layout_gravity="center">
		    	<Button
		        android:id="@+id/cmdEventSave"
		        android:layout_width="150sp"
		        android:layout_height="wrap_content"
		        android:layout_gravity="left"
		        android:layout_marginTop="10dp"
		        android:text="Save" />		

		    			    					    
				<Button
		        android:id="@+id/close"
		        android:layout_width="150sp"
		        android:layout_height="wrap_content"
		        android:layout_gravity="right"
		        android:layout_marginTop="10dp"
		        android:text="Close" />		
	        
		</LinearLayout>
		
</LinearLayout>